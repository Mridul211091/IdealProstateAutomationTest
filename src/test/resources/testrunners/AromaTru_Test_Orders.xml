<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="AromaTru Order Flow Tests" verbose="3"
	thread-count="1" parallel="tests">

<listeners>
	
		<listener
			class-name="agi.qa.aromatru.listeners.ExtentReportListener" />

<listener
			class-name="agi.qa.aromatru.listeners.TestAllureListener" />
			
	</listeners>

	
	<test name="Place Test Orders">
		<parameter name="browser" value="chrome" />

		<classes>
			<class name="agi.qa.aromatru.tests.AromaTruOrderFlowTest" />
		</classes>
	</test>


</suite>