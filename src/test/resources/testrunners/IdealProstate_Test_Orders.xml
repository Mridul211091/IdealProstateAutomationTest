<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="IdealProstate Order Flow Tests" verbose="3"
	thread-count="1" parallel="tests">

<listeners>
	
		<listener
			class-name="agi.qa.idealprostate.listeners.ExtentReportListener" />

<listener
			class-name="agi.qa.idealprostate.listeners.TestAllureListener" />
			
	</listeners>

	
	<test name="Place Test Orders">
		<parameter name="browser" value="chrome" />

		<classes>
			<class name="agi.qa.idealprostate.tests.IdealProstateOrderFlowTest" />
		</classes>
	</test>


</suite>